<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans" 
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:context="http://www.springframework.org/schema/context"
    xmlns:aop="http://www.springframework.org/schema/aop"
    xmlns:tx="http://www.springframework.org/schema/tx"
    xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-2.0.xsd
    http://www.springframework.org/schema/context http://www.springframework.org/schema/context/spring-context-2.5.xsd
    http://www.springframework.org/schema/aop http://www.springframework.org/schema/aop/spring-aop-2.0.xsd
    http://www.springframework.org/schema/tx http://www.springframework.org/schema/tx/spring-tx-2.0.xsd"
    default-lazy-init="true">
    
    <bean id="workerPoolFactory" class="org.gbif.scheduler.mock.WorkerPoolFactory">
        <constructor-arg ref="jobDao"/>
    </bean>
    <bean id="mockInstance" class="org.gbif.scheduler.mock.MockInstance"/>
    <bean id="mockJob" class="org.gbif.scheduler.mock.MockJob">
        <constructor-arg ref="mockInstance"/>
    </bean>    
    
