<?xml version="1.0" encoding="UTF-8"?>
<!-- 
	This defines the extension for defining vernacular names as extensions to a Taxon core object
	An instance of this represents a http://rs.gbif.org/ecat/class/VernacularName
	
	This has the dependency on thesauri:
		- language code:  http://rs.gbif.org/thesaurus/languageCode.xml
-->
<extension xmlns="http://ipt.gbif.org/extension/"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:dc="http://purl.org/dc/terms/"
    xsi:schemaLocation="http://ipt.gbif.org/extension/ http://gbrds.gbif.org/resources/schemas/extension.xsd" 
    title="Species Status" name="SpeciesStatus" namespace="http://www.gisinetwork.org/IASProfile">
    <property name="Abundance" namespace="http://www.gisinetwork.org/IASProfile" qualName="http://www.gisinetwork.org/IASProfile/Abundance" thesaurus="http://gbrds.gbif.org/resources/thesauri/gisin-abundance.xml" columnLength="128" dc:description="http://www.niiss.org/cwis438/websites/GISINDirectory/tech/Concept_Info.php?ConceptID=5"/>
    <property name="Distribution" namespace="http://www.gisinetwork.org/IASProfile" qualName="http://www.gisinetwork.org/IASProfile/Distribution" thesaurus="http://gbrds.gbif.org/resources/thesauri/gisin-distribution.xml" columnLength="128" dc:description="http://www.niiss.org/cwis438/websites/GISINDirectory/tech/Concept_Info.php?ConceptID=4"/>
    <property name="Harmful" namespace="http://www.gisinetwork.org/IASProfile" qualName="http://www.gisinetwork.org/IASProfile/Harmful" thesaurus="http://gbrds.gbif.org/resources/thesauri/gisin-harmful.xml" columnLength="128" dc:description="http://www.niiss.org/cwis438/websites/GISINDirectory/tech/Concept_Info.php?ConceptID=8"/>
    <property name="LocalityStandard" namespace="http://www.gisinetwork.org/IASProfile" qualName="http://www.gisinetwork.org/IASProfile/LocalityStandard" columnLength="128" dc:description="http://www.niiss.org/cwis438/websites/GISINDirectory/tech/Concept_Info.php?ConceptID=97"/>
    <property name="Origin" namespace="http://www.gisinetwork.org/IASProfile" qualName="http://www.gisinetwork.org/IASProfile/Origin" thesaurus="http://gbrds.gbif.org/resources/thesauri/gisin-origin.xml" columnLength="128" dc:description="http://www.niiss.org/cwis438/websites/GISINDirectory/tech/Concept_Info.php?ConceptID=1"/>
    <property name="Persistence" namespace="http://www.gisinetwork.org/IASProfile" qualName="http://www.gisinetwork.org/IASProfile/Persistence" thesaurus="http://gbrds.gbif.org/resources/thesauri/gisin-persistence.xml" columnLength="128" dc:description="http://www.niiss.org/cwis438/websites/GISINDirectory/tech/Concept_Info.php?ConceptID=3"/>
    <property name="Presence" namespace="http://www.gisinetwork.org/IASProfile" qualName="http://www.gisinetwork.org/IASProfile/Presence" thesaurus="http://gbrds.gbif.org/resources/thesauri/gisin-presence.xml" columnLength="128" dc:description="http://www.niiss.org/cwis438/websites/GISINDirectory/tech/Concept_Info.php?ConceptID=2"/>
    <property name="PublicationDate" namespace="http://www.gisinetwork.org/IASProfile" qualName="http://www.gisinetwork.org/IASProfile/PublicationDate" columnLength="128" dc:description="http://www.niiss.org/cwis438/websites/GISINDirectory/tech/Concept_Info.php?ConceptID=93"/>
    <property name="RateOfSpread" namespace="http://www.gisinetwork.org/IASProfile" qualName="http://www.gisinetwork.org/IASProfile/RateOfSpread" thesaurus="http://gbrds.gbif.org/resources/thesauri/gisin-spreadrate.xml" columnLength="128" dc:description="http://www.niiss.org/cwis438/websites/GISINDirectory/tech/Concept_Info.php?ConceptID=7"/>
    <property name="RegulatoryListing" namespace="http://www.gisinetwork.org/IASProfile" qualName="http://www.gisinetwork.org/IASProfile/RegulatoryListing" thesaurus="http://gbrds.gbif.org/resources/thesauri/gisin-regulatory.xml" columnLength="128" dc:description="http://www.niiss.org/cwis438/websites/GISINDirectory/tech/Concept_Info.php?ConceptID=9"/>
    <property name="Trend" namespace="http://www.gisinetwork.org/IASProfile" qualName="http://www.gisinetwork.org/IASProfile/Trend" thesaurus="http://gbrds.gbif.org/resources/thesauri/gisin-trend.xml" columnLength="128" dc:description="http://www.niiss.org/cwis438/websites/GISINDirectory/tech/Concept_Info.php?ConceptID=6"/>
    
    <property name="Continent" namespace="http://rs.tdwg.org/dwc/terms/" qualName="http://rs.tdwg.org/dwc/terms/Continent" columnLength="64"/>
    <property name="Waterbody" namespace="http://rs.tdwg.org/dwc/terms/" qualName="http://rs.tdwg.org/dwc/terms/Waterbody" columnLength="128"/>
    <property name="IslandGroup" namespace="http://rs.tdwg.org/dwc/terms/" qualName="http://rs.tdwg.org/dwc/terms/IslandGroup" columnLength="128"/>
    <property name="Island" namespace="http://rs.tdwg.org/dwc/terms/" qualName="http://rs.tdwg.org/dwc/terms/Island" columnLength="128"/>
    <property name="Country" namespace="http://rs.tdwg.org/dwc/terms/" qualName="http://rs.tdwg.org/dwc/terms/Country" columnLength="128"/>
    <property name="CountryCode" namespace="http://rs.tdwg.org/dwc/terms/" qualName="http://rs.tdwg.org/dwc/terms/CountryCode" thesaurus="http://gbrds.gbif.org/resources/thesauri/country-code.xml" columnLength="3"/>
    <property name="StateProvince" namespace="http://rs.tdwg.org/dwc/terms/" qualName="http://rs.tdwg.org/dwc/terms/StateProvince" columnLength="128"/>
    <property name="County" namespace="http://rs.tdwg.org/dwc/terms/" qualName="http://rs.tdwg.org/dwc/terms/County" columnLength="128"/>
    <property name="Locality" namespace="http://rs.tdwg.org/dwc/terms/" qualName="http://rs.tdwg.org/dwc/terms/Locality" columnLength="256"/>
    
    <property name="TaxonRemarks" namespace="http://rs.tdwg.org/dwc/terms/" qualName="http://rs.tdwg.org/dwc/terms/TaxonRemarks" columnLength="1000"/>    
</extension>