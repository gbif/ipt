<?xml version="1.0" encoding="UTF-8"?>
<!-- 
	This defines the extension for defining vernacular names as extensions to a Taxon core object
	
	This has the dependency on thesauri:
	- language code:  http://gbrds.gbif.org/resources/thesauri/lang.xml
	- country codes:  http://gbrds.gbif.org/resources/thesauri/country-code.xml
	
-->
<extension xmlns="http://ipt.gbif.org/extension/"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://ipt.gbif.org/extension/ http://gbrds.gbif.org/resources/schemas/extension.xsd" 
    title="GNA Literature References" name="Reference" namespace="http://rs.gbif.org/terms/1.0">
    
    <property name="identifier" namespace="http://purl.org/dc/terms/" qualName="http://purl.org/dc/terms/identifier" />
    <property name="bibliographicCitation" namespace="http://purl.org/dc/terms/" qualName="http://purl.org/dc/terms/bibliographicCitation"/>
    <property name="title" namespace="http://purl.org/dc/terms/" qualName="http://purl.org/dc/terms/title"/>
    <property name="creator" namespace="http://purl.org/dc/terms/" qualName="http://purl.org/dc/terms/creator"/>
    <property name="date" namespace="http://purl.org/dc/terms/" qualName="http://purl.org/dc/terms/date"/>
    <property name="source" namespace="http://purl.org/dc/terms/" qualName="http://purl.org/dc/terms/source" />
    <property name="description" namespace="http://purl.org/dc/terms/" qualName="http://purl.org/dc/terms/description"/>
    <property name="subject" namespace="http://purl.org/dc/terms/" qualName="http://purl.org/dc/terms/subject"/>
    <property name="language" namespace="http://purl.org/dc/terms/" qualName="http://purl.org/dc/terms/language" thesaurus="http://gbrds.gbif.org/resources/thesauri/lang.xml" columnLength="2" />
    <property name="rights" namespace="http://purl.org/dc/terms/" qualName="http://purl.org/dc/terms/rights"/>
    <property name="taxonRemarks" namespace="http://rs.tdwg.org/dwc/terms/" qualName="http://rs.tdwg.org/dwc/terms/taxonRemarks" />
    <property name="type" namespace="http://purl.org/dc/terms/" qualName="http://purl.org/dc/terms/type"/>

</extension>