							<implementation>annotationconfiguration</implementation>
							<outputDirectory>target</outputDirectory>
						</component>
					</components>
					<componentProperties>
						<jdk5>true</jdk5>
						<drop>true</drop>
						<create>true</create>
						<format>true</format>
						<export>false</export>
						<propertyfile>target/classes/application.properties</propertyfile>
						<outputfilename>ipt-ddl.sql</outputfilename>
