				<includes>
					<include>applicationContext-resources.xml</include>
					<include>struts.xml</include>
					<include>ApplicationResources_de.properties</include>
					<include>ApplicationResources_es.properties</include>
					<include>ApplicationResources_fr.properties</include>
					<include>ApplicationResources_kr.properties</include>
				</includes>
				<filtering>false</filtering>
			</resource>
		</resources>
		<testResources>
