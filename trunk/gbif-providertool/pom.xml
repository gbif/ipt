				            <contextPath>/</contextPath>
					<scanTargetPatterns>
						<scanTargetPattern>
							<directory>src/main/webapp</directory>
							<includes>
								<include>**/*</include>
							</includes>
						</scanTargetPattern>
					</scanTargetPatterns>
					<scanIntervalSeconds>2</scanIntervalSeconds>
				</configuration>

