<!DOCTYPE hibernate-configuration PUBLIC "-//Hibernate/Hibernate Configuration DTD 3.0//EN"
    "http://hibernate.sourceforge.net/hibernate-configuration-3.0.dtd">

<hibernate-configuration>
    <session-factory>
        <property name="hibernate.ejb.naming_strategy">${hibernate.namingStrategy}</property>

        <mapping class="org.appfuse.model.User"/>
        <mapping class="org.appfuse.model.Role"/>
        <mapping class="org.gbif.provider.model.Annotation"/>
        <mapping class="org.gbif.provider.model.ChecklistResource"/>
        <mapping class="org.gbif.provider.model.DataResource"/>
        <mapping class="org.gbif.provider.model.Extension"/>
        <mapping class="org.gbif.provider.model.ExtensionMapping"/>
        <mapping class="org.gbif.provider.model.ExtensionProperty"/>
        <mapping class="org.gbif.provider.model.OccurrenceResource"/>
        <mapping class="org.gbif.provider.model.PropertyMapping"/>
        <mapping class="org.gbif.provider.model.ProviderCfg"/>
        <mapping class="org.gbif.provider.model.Resource"/>
        <mapping class="org.gbif.provider.model.ResourceMetadata"/>
        <mapping class="org.gbif.provider.model.SourceBase"/>
        <mapping class="org.gbif.provider.model.SourceSql"/>
        <mapping class="org.gbif.provider.model.SourceFile"/>
        <mapping class="org.gbif.provider.model.TermMapping"/>
        <mapping class="org.gbif.provider.model.ThesaurusConcept"/>
        <mapping class="org.gbif.provider.model.ThesaurusTerm"/>
        <mapping class="org.gbif.provider.model.ThesaurusVocabulary"/>
        <mapping class="org.gbif.provider.model.Transformation"/>
        <mapping class="org.gbif.provider.model.UploadEvent"/>
    </session-factory>
</hibernate-configuration>