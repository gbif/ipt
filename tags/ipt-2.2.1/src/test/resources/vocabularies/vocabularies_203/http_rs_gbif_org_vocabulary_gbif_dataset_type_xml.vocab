    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://rs.gbif.org/thesaurus/  http://rs.gbif.org/schema/thesaurus.xsd">
    <concept dc:identifier="occurrence" dc:URI="http://rs.gbif.org/vocabulary/gbif/datasetType/occurrence"
        dc:relation=""
        dc:description="a generic occurrence resource.">
        <preferred>
            <term dc:title="Occurrence" xml:lang="en"/>
            <term dc:title="Ocurrencia" xml:lang="es"/>
            <term dc:title="Occurrence" xml:lang="fr"/>
        </preferred>
        <alternative>
        </alternative>
